<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:tools="http://schemas.android.com/tools"
             android:layout_width="match_parent"
             android:layout_height="match_parent"
              android:orientation="vertical"
             >

    <android.support.design.widget.TextInputEditText
        android:id="@+id/edit_house_name"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="输入姓名"/>

    <android.support.design.widget.TextInputEditText
        android:id="@+id/edit_house_erea"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="性别"/>

    <android.support.design.widget.TextInputEditText
    android:id="@+id/edit_men_cardid"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:hint="身份证"/>

    <android.support.design.widget.TextInputEditText
        android:id="@+id/edit_man_house_num"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="输入房号"/>

    <android.support.design.widget.TextInputEditText
        android:id="@+id/edit_man_phone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="输入电话"/>

    <android.support.design.widget.TextInputEditText
        android:id="@+id/edit_man_begin"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="起始日期"/>

    <android.support.design.widget.TextInputEditText
        android:id="@+id/edit_man_end"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="截止日期"/>

    <android.support.design.widget.TextInputEditText
        android:id="@+id/edit_man_pay"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="交房租日期"/>



</LinearLayout>
